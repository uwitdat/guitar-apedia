<h2>Seach Guitars</h2>
<form action='/guitars'  method='GET'>
    <div>
        <label>Name</label>
        <input type='text' name='name' value='<%= searchOptions.name %>'> 
    </div>
    <button type="submit">Search</button>
</form>
<br>
<div>
<% guitars.forEach(guitar => { %>
    <img height='150' width='100' src='<%= guitar.imagePath %>'>
<% }); %>
</div>